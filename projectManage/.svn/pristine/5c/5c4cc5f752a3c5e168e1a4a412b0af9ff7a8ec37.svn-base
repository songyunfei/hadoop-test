<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linkGap.projectManage.dao.UserMapper1">
	<resultMap id="user_map" type="com.linkGap.projectManage.model.entity.User">
		<id property="id" column="id" jdbcType="INTEGER"></id>
		<result property="userId" column="userId" jdbcType="INTEGER"></result>
		<result property="userName" column="userName" jdbcType="VARCHAR"></result>
		<result property="age" column="age" jdbcType="INTEGER"></result>
		<result property="email" column="email" jdbcType="VARCHAR"></result>
	</resultMap>
	<select id="findAll" resultMap="user_map"> 
	select userName,age from user
	</select>
</mapper>