      contractImageUrls = #{contractImageUrls,jdbcType=VARCHAR},
      constructionShortImageUrls = #{constructionShortImageUrls,jdbcType=VARCHAR},
      constructionImageUrls = #{constructionImageUrls,jdbcType=VARCHAR},
      saleMan = #{saleMan,jdbcType=INTEGER},
      designMan = #{designMan,jdbcType=INTEGER},
      constructLeader = #{constructLeader,jdbcType=INTEGER},
      supervisor = #{supervisor,jdbcType=INTEGER},
      currentProgress = #{currentProgress,jdbcType=VARCHAR},
      score = #{score,jdbcType=VARCHAR},
      createTime = #{createTime,jdbcType=TIMESTAMP},
      createUserId = #{createUserId,jdbcType=INTEGER},
      updateTime = #{updateTime,jdbcType=TIMESTAMP},
      updateUserId = #{updateUserId,jdbcType=INTEGER},
