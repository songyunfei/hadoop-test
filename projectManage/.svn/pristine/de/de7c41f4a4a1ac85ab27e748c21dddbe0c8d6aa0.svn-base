      buildingName = #{buildingName,jdbcType=VARCHAR},
      addressDetail = #{addressDetail,jdbcType=VARCHAR},
      customerName = #{customerName,jdbcType=VARCHAR},
      customerMobile = #{customerMobile,jdbcType=VARCHAR},
