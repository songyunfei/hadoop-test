  <select id="queryList" resultMap="ViewResultMap" parameterType="com.linkGap.projectManage.model.view.ProjectRequestParams" >
    select 
    p.projectInfoId,p.buildingName,p.createTime,p.currentProgress,
    p.projectType,p.customerName,cs.shopName as customerShopName,
    su1.username as constructLeader,su2.username as supervisor,
    su3.username as saleMan,
    IF(p.projectStatus=1,'灰',IF(p.projectStatus=3,'绿',IF(p.status=1,'黄',IF(p.status=2,'红',IF(p.status=3,'蓝','无色'))))) AS colorShow  
    from project_info p 
         left join customer_shops cs on p.customerShopId=cs.customerShopId
         left join sys_user su1 on p.constructLeader=su1.sysUserId
         left join sys_user su2 on p.supervisor=su2.sysUserId
         left join sys_user su3 on p.saleMan=su3.sysUserId
         left join sys_user su4 on p.saleMan=su3.sysUserId
    <where>
         <if test="userId!= null" >
	       and ( p.createUserId=#{userId} or p.saleMan=#{userId} or p. designMan=#{userId} or p.constructLeader=#{userId} or p.supervisor=#{userId})
	     </if>
	     <if test="listType!= null" >
	       and  p.projectStatus = #{listType}
	     </if>
	     <if test="searchContent!= null" >
	       and  (p.buildingName like "%"||#{searchContent}||"%" or cs.showName like "%"||#{searchContent}||"%") 
	     </if>
	     <if test="projectType!= null" >
	       and  p.projectType =#{projectType}
	     </if>
	     <if test="customerShopId!= null" >
	       and cs.customerShopId = #{customerShopId}
	     </if>
	      <if test="constructLeader!= null" >
	       and su1.sysUserId=#{constructLeader}
	     </if>
	</where>
	<if test="progressOrder== 0" >
	      order by p.updateTime desc
    </if>
    <if test="progressOrder== 1" >
	      order by CAST('op.currentProgress' AS SIGNED) asc
    </if>
    <if test="progressOrder== 2" >
	      order by CAST('op.currentProgress' AS SIGNED) desc
    </if>  
    limit #{pageNub},#{pageSize}
  </select>
  <!--查询总数  -->
  <select id="getTotalByParams" resultMap="ViewResultCountMap" parameterType="com.linkGap.projectManage.model.view.ProjectRequestParams" >
   
   SELECT DISTINCT pi.projectType as type, IFNULL( tb.count, 0) count  
        FROM project_info pi
        LEFT JOIN(
		    select 
		    p.projectType ,count(projectInfoId) as count
		    from project_info p 
		         left join customer_shops cs on p.customerShopId=cs.customerShopId
		         left join sys_user su1 on p.constructLeader=su1.sysUserId
		         left join sys_user su2 on p.supervisor=su2.sysUserId
		         left join sys_user su3 on p.saleMan=su3.sysUserId
		         left join sys_user su4 on p.saleMan=su3.sysUserId
		    <where>
		         <if test="userId!= null" >
			       and ( p.createUserId=#{userId} or p.saleMan=#{userId} or p. designMan=#{userId} or p.constructLeader=#{userId} or p.supervisor=#{userId})
			     </if>
			     <if test="listType!= null" >
			       and  p.projectStatus = #{listType}
			     </if>
			     <if test="searchContent!= null" >
			       and  (p.buildingName like "%"||#{searchContent}||"%" or cs.showName like "%"||#{searchContent}||"%") 
			     </if>
			     <if test="projectType!= null" >
			       and  p.projectType =#{projectType}
			     </if>
			     <if test="customerShopId!= null" >
			       and cs.customerShopId = #{customerShopId}
			     </if>
			      <if test="constructLeader!= null" >
			       and su1.sysUserId=#{constructLeader}
			     </if>
			</where>
			group by p.projectType) as tb
	        on pi.projectType =tb.projectType
  </select>
