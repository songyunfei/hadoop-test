spring:
  datasource:
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://localhost:3306/ProjectManage?useUnicode=true&characterEncoding=UTF-8
    username: root
    password: root
    druid: 
      stat-view-servlet:
        #allow: 127.0.0.1 #ip白名单，deny ip黑名单，缺省允许所有ip访问
        login-username: admin #登录用户名
        login-password: projectManage_linkgap #登录密码
        url-pattern: /druid/* #拦截URL，默认配置就是/druid/*
      web-stat-filter:
        exclusions: '*.js,*.gif,*.jpg,*.bmp,*.png,*.css,*.ico,*.ttf,*.svg,/druid/*' #剔除过滤，默认过滤/*
        principal-session-name: activeUser #如果用户登录session中存储用户，可以在session监控显示登录用户名，如果session中放的是对象，需要重新toString()方法
        session-stat-enable: true #开启session监控
      filters: wall,stat,log4j #过滤器
      initial-size: 5 #初始化大小
      min-idle: 2 #最小
      max-active: 30 #最大
      enable: true #开启druid
      filter:
        stat:
          merge-sql: true #合并sql，例如select * from user where id =1;select * from user where id =2;合并后select * from user where id =?
  redis:
    database: 2
    host: 139.196.36.116
    port: 6379
    password: linkGap
    pool:
      max-idle: 300
      min-idle: 0
      max-active: 600
      max-wait: 3000
    timeout: 6000
upload:
  image:
    path: D:/upload/projectManage/imgage
  file: 
    path: D:/upload/projectManage/file
account: 
  maximumSessions: 2
environment:
  active: 